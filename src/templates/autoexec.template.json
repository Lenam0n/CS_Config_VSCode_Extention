  "sections": {
    "01": {
      "title": "[01] Core Settings",
      "lines": [
        "con_enable \"1\"",
        "fps_max \"0\"                // 0 = uncap, else a hard cap",
        "mm_dedicated_search_maxping \"60\"    // max acceptable matchmaking ping",
        "cl_showloadout \"1\"",
        "cl_teamid_overhead_mode \"2\" // clearer teammate pips/names",
        "",
        "echo \"[autoexec] Core Settings loaded\""
      ]
    },
    "02": {
      "title": "[02] HUD & Viewmodel",
      "lines": [
        "// Viewmodel (adjust to your preference)",
        "viewmodel_fov \"68\"",
        "viewmodel_offset_x \"2.5\"",
        "viewmodel_offset_y \"0\"",
        "viewmodel_offset_z \"-1.5\"",
        "",
        "// HUD (radar is in its own section below)",
        "hud_scaling \"0.95\"",
        "",
        "echo \"[autoexec] HUD & Viewmodel loaded\""
      ]
    },
    "03": {
      "title": "[03] Audio",
      "lines": [
        "volume \"0.7\"",
        "voice_enable \"1\"",
        "voice_scale \"0.6\"",
        "",
        "echo \"[autoexec] Audio loaded\""
      ]
    },
    "04": {
      "title": "[04] Radar",
      "lines": [
        "cl_radar_always_centered \"0\"",
        "cl_radar_scale \"0.9\"",
        "cl_radar_icon_scale_min \"0.8\"",
        "",
        "echo \"[autoexec] Radar loaded\""
      ]
    },
    "05": {
      "title": "[05] Mouse & Sensitivity",
      "lines": [
        "m_rawinput \"1\"",
        "m_customaccel \"0\"",
        "sensitivity \"1.50\"",
        "zoom_sensitivity_ratio \"1.0\"",
        "",
        "echo \"[autoexec] Mouse & Sens loaded\""
      ]
    },

