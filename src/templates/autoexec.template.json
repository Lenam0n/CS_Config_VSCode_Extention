    "06E": {
      "title": "[06E] Binds — Keyboard (Punctuation / OEM)",
      "lines": [
        "bind \"grave\" \"\"        // `~",
        "bind \"minus\" \"\"        // -_",
        "bind \"equals\" \"\"       // =+",
        "bind \"leftbracket\" \"\"  // [{",
        "bind \"rightbracket\" \"\" // ]}",
        "bind \"semicolon\" \"\"     // ;:",
        "bind \"apostrophe\" \"\"    // '\"",
        "bind \"comma\" \"\"         // ,<",
        "bind \"period\" \"\"        // .>",
        "bind \"slash\" \"\"         // /?",
        "bind \"backslash\" \"\"     // \\|",
        "",
        "echo \"[autoexec] Keyboard (Punctuation/OEM) ready\""
      ]
    },

